# AWS Cognito Configuration
# Copy this file to .env and replace with your actual values
# All React environment variables must start with REACT_APP_

# Development server port
PORT=3000

REACT_APP_AWS_REGION=your-aws-region
REACT_APP_COGNITO_USER_POOL_ID=your-user-pool-id
REACT_APP_COGNITO_USER_POOL_WEB_CLIENT_ID=your-web-client-id
REACT_APP_COGNITO_DOMAIN=your-domain.auth.region.amazoncognito.com
REACT_APP_PORT=3000
REACT_APP_REDIRECT_SIGN_IN=http://localhost:3000/
REACT_APP_REDIRECT_SIGN_OUT=http://localhost:3000/
